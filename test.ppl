(i32 n) -> i32 factorial
{
    i32 fact = 1;

    i32 i = 1;
    while(i <= n)
    {
        fact = fact * i;
        ++i;
    }

    return fact;
}

() -> i32 main
{
    i32[2] n = 0;
    n[0] = 9;
    n[1] = factorial(n[0]);
    f32 f = n as f32;
    dump("{}! = {}", n[0], n[1]);
    return 0;
}

